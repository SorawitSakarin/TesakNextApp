{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": ["javascript", "typescript"],
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "[javascript]": {
      "editor.tabSize": 2
    },
    "[javascriptreact]": {
      "editor.tabSize": 2
    },
    "[typescript]": {
      "editor.tabSize": 2
    },
    "[typescriptreact]": {
      "editor.tabSize": 2
    }
  }
  